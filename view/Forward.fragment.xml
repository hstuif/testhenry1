<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <SelectDialog
    title="Users"
    class="sapUiPopupWithPadding"
    items="{users>/ET_USERS}"
    confirm="_handleFWClose"
    cancel="_handleFWClose">
    <StandardListItem
      title="{users>UNAME}"
      description="{users>FIRST} + ' ' + {users>LAST}" />
  </SelectDialog>
</core:FragmentDefinition>

